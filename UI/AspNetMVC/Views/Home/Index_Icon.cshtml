            font-size: 18px;
        }

        .icondemoFontIconsWithLigatures .material-icons.md-24 {
            font-size: 24px;
        }

        .icondemoFontIconsWithLigatures .material-icons.md-36 {
            font-size: 36px;
        }

        .icondemoFontIconsWithLigatures .material-icons.md-48 {
            font-size: 48px;
        }

        .icondemoFontIconsWithLigatures .material-icons.md-dark {
            color: rgba(0, 0, 0, 0.54);
        }

            .icondemoFontIconsWithLigatures .material-icons.md-dark.md-inactive {
                color: rgba(0, 0, 0, 0.26);
            }

        .icondemoFontIconsWithLigatures .material-icons.md-light {
            color: white;
        }

            .icondemoFontIconsWithLigatures .material-icons.md-light.md-inactive {
                color: rgba(255, 255, 255, 0.3);
