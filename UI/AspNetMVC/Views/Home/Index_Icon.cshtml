<style>
    .icondemoFontIconsWithLigatures {
        /* Rules for sizing the icon.*/
        /* Rules for using icons as black on a light background.*/
        /* Rules for using icons as white on a dark background.*/
    }

        .icondemoFontIconsWithLigatures .appDemoFontIconsWithLigatures {
            padding: 25px;
            width: 100%;
        }

            .icondemoFontIconsWithLigatures .appDemoFontIconsWithLigatures,
            .icondemoFontIconsWithLigatures .appDemoFontIconsWithLigatures *:before,
            .icondemoFontIconsWithLigatures .appDemoFontIconsWithLigatures *:after {
                box-sizing: border-box;
            }

        .icondemoFontIconsWithLigatures header {
            overflow: hidden;
        }

            .icondemoFontIconsWithLigatures header h1 {
                color: #888;
                font-size: 36px;
                font-weight: 300;
            }

        .icondemoFontIconsWithLigatures .container {
            margin: 0 auto;
            max-width: 1200px;
            min-width: 960px;
            padding: 40px 40px;
            font: 14px/1.5 "Helvetica Neue", Helvetica, Arial, sans-serif;
        }

        .icondemoFontIconsWithLigatures .glyph {
            border-bottom: 1px dotted #ccc;
            padding: 10px 0 20px;
            margin-bottom: 20px;
        }

